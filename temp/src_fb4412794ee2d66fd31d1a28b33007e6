$arrAljon = array('only chase power to steal from us','are unfairly abused because they all try their hardest at an impossible job','get into politics with good intention but are corrupted by the system','I’m unsure'); 

 
  

 
foreach ($arrAljon as $value) { 
    echo "Value: $value <br/>\n"; 

 
   $count2a = $db->getQuery(true); 

 
  

 
$count2a->select('COUNT(*)'); 

 
  

 
$count2a->from($db->quoteName('jos_rsform_submission_values'));  

 
  

 
$count2a->where($db->quoteName('FieldName') ."=". "'"."Politicians"."'"); 

 
  

 
$count2a->where($db->quoteName('FormId') ." = ". '61');  

 
  

 
$count2a->where($db->quoteName('FieldValue') ." LIKE ". "'%".{$value}."%'");  

 
  

 
$db->setQuery($count2a); 

 
  

 
$count_aljon= $db->loadResult(); 

 
echo $count_aljon 
};
;return get_defined_vars();
