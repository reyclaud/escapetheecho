ESC" />				<field name="module_posts_fb_height" type="text" size="5" default="500" label="COM_INSTANTFBLOGIN_MODULEPOSTS_FB_HEIGHT" description="COM_INSTANTFBLOGIN_MODULEPOSTS_FB_HEIGHT_DESC" />				<field name="module_posts_fb_tabs" type="text" size="5" default="timeline" label="COM_INSTANTFBLOGIN_MODULEPOSTS_FB_TABS" description="COM_INSTANTFBLOGIN_MODULEPOSTS_FB_TABS_DESC" />				<field name="module_posts_fb_hide_cover" type="radio" class="btn-group btn-group-yesno" default="0" label="COM_INSTANTFBLOGIN_MODULEPOSTS_FB_HIDE_COVER" description="COM_INSTANTFBLOGIN_MODULEPOSTS_FB_HIDE_COVER_DESC">					<option value="1">JYES</option>					<option value="0">JNO</option>				</field>				<field name="module_posts_fb_show_facepile" type="radio" class="btn-group btn-group-yesno" default="1" label="COM_INSTANTFBLOGIN_MODULEPOSTS_FB_SHOW_FACEPILE" description="COM_INSTANTFBLOGIN_MODULEPOSTS_FB_SHOW_FACEPILE_DESC">					<option value="1">JYES</option>					<option value="0">JNO</option>				</field>				<field name="module_posts_fb_small_header" type="radio" class="btn-group btn-group-yesno" default="0" label="COM_INSTANTFBLOGIN_MODULEPOSTS_FB_SMALL_HEADER" description="COM_INSTANTFBLOGIN_MODULEPOSTS_FB_SMALL_HEADER_DESC">					<option value="1">JYES</option>					<option value="0">JNO</option>				</field>				<field name="module_posts_fb_adapt_container_width" type="radio" class="btn-group btn-group-yesno" default="1" label="COM_INSTANTFBLOGIN_MODULEPOSTS_FB_ADAPT_CONTAINER_WIDTH" description="COM_INSTANTFBLOGIN_MODULEPOSTS_FB_ADAPT_CONTAINER_WIDTH_DESC">					<option value="1">JYES</option>					<option value="0">JNO</option>				</field>			</fieldset>						<fieldset name="google" label="COM_INSTANTFBLOGIN_MODULEPOSTS_GOOGLE_SETTINGS">				<field name="module_posts_google_enabled" type="radio" class="btn-group btn-group-yesno" default="1" label="COM_INSTANTFBLOGIN_MODULEPOSTS_GOOGLE_ENABLED" description="COM_INSTANTFBLOGIN_MODULEPOSTS_GOOGLE_ENABLED_DESC">					<option value="1">JYES</option>					<option value="0">JNO</option>				</field>				<field name="module_posts_google_apikey" type="text" size="50" default="AIzaSyCUGl_fXs5TEABpAVLEimbu4yPsgMGxTI8" label="COM_INSTANTFBLOGIN_MODULEPOSTS_GOOGLE_APIKEY" description="COM_INSTANTFBLOGIN_MODULEPOSTS_GOOGLE_APIKEY_DESC" />				<field name="module_posts_google_user" type="text" size="50" default="+google" label="COM_INSTANTFBLOGIN_MODULEPOSTS_GOOGLE_USER" description="COM_INSTANTFBLOGIN_MODULEPOSTS_GOOGLE_USER_DESC" />				<field name="module_posts_google_maxresults" type="text" size="50" default="10" label="COM_INSTANTFBLOGIN_MODULEPOSTS_GOOGLE_MAXRESULTS" description="COM_INSTANTFBLOGIN_MODULEPOSTS_GOOGLE_MAXRESULTS_DESC" />				<field name="module_posts_google_width" type="text" size="5" default="340" label="COM_INSTANTFBLOGIN_MODULEPOSTS_GOOGLE_WIDTH" description="COM_INSTANTFBLOGIN_MODULEPOSTS_GOOGLE_WIDTH_DESC" />				<field name="module_posts_google_height" type="text" size="5" default="500" label="COM_INSTANTFBLOGIN_MODULEPOSTS_GOOGLE_HEIGHT" description="COM_INSTANTFBLOGIN_MODULEPOSTS_GOOGLE_HEIGHT_DESC" />			</fieldset>						<fieldset name="twitter" label="COM_INSTANTFBLOGIN_MODULEPOSTS_TWITTER_SETTINGS">				<field name="module_posts_twitter_enabled" type="radio" class="btn-group btn-group-yesno" default="1" label="COM_INSTANTFBLOGIN_MODULEPOSTS_TWITTER_ENABLED" description="COM_INSTANTFBLOGIN_MODULEPOSTS_TWITTER_ENABLED_DESC">					<option value="1">JYES</option>					<option value="0">JNO</option>				</field>				<field name="module_posts_twitter_widgetid" type="text" size="50" default="600720083413962752" label="COM_INSTANTFBLOGIN_MODULEPOSTS_TWITTER_WIDGETID" description="COM_INSTANTFBLOGIN_MODULEPOSTS_TWITTER_WIDGETID_DESC" />				<field name="module_posts_twitter_user" type="text" size="50" default="TwitterDev" label="COM_INSTANTFBLOGIN_MODULEPOSTS_TWITTER_USER" description="COM_INSTANTFBLOGIN_MODULEPOSTS_TWITTER_USER_DESC" />				<field name="module_posts_t